<%= simple_form_for(@institucion) do |f| %>
  <%= f.error_notification %>
  <div class="field">
      <%= '       Los Campos con (*) son Obligatorios!' %><br>
      </div>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :descripcion %>
    <%= f.input :mision %>
    <%= f.input :vision %>
    <%= f.input :organigrama %>
    <%= f.input :objetivos %>
    <%= f.input :logo %>
    <%= f.input :tipo, collection: ['Funsone','Paz y Vida'], prompt:"Seleccione el tipo"%>
    <%= f.input :foto1 %>
    <%= f.input :foto2 %>
    <%= f.input :foto3 %>
    <%= f.input :foto4 %>
    <%= f.input :foto5 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
