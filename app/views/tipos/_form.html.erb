<%= simple_form_for(@tipo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre_ayuda %>
    <%= f.input :objetivo %>
    <%= f.input :requisito %>
    <%= f.input :tipo %>
    <%= f.input :contactos %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
