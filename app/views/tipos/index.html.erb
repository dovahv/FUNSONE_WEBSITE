<p id="notice"><%= notice %></p>

<h1>Listing Tipos</h1>

<table>
  <thead>
    <tr>
       <th>Nombre</th>
      <th>Objetivo</th>
      <th>Requisito</th>
      <th>Tipo</th>
      <th>Contactos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos.each do |tipo| %>
      <tr>
        <td><%= tipo.nombre_ayuda %></td>
        <td><%= tipo.objetivo %></td>
        <td><%= tipo.requisito %></td>
        <td><%= tipo.tipo %></td>
        <td><%= tipo.contactos %></td>
        <td><%= link_to 'Show', tipo %></td>
        <td><%= link_to 'Edit', edit_tipo_path(tipo) %></td>
        <td><%= link_to 'Destroy', tipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo', new_tipo_path %>
